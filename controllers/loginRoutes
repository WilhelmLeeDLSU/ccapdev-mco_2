
module.exports.add = (server) => {
    server.get('/login', function(req, resp){
        resp.render('login',{
            layout: 'index',
            title: 'Login',
        });
    });
    
    server.get('/register', function(req, resp){
        resp.render('register',{
            layout: 'index',
            title: 'Register', 
        });
    });
}